const int mx = 1e6+123;
bitset<mx>is_prime;
vector<int>primes;

void PrimeGen(int n){
  for(int i = 3;i<=n;i+=2) is_prime[i] = 1;
  
  for(int i = 3;i<=sqrt(n);i+=2){
    for(int j = i*i;j<=n;j+=i){
      is_prime[j] = 0;
      }
    }
  
  is_prime[2] = 1;
  primes.push_back(2);
  
  for(int i = 3;i<=n;i+=2){
  if(is_prime[i])
    primes.push_back(i);
    
  }
}
